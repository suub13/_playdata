{{>layout/header}}
<div class="container">
    <div class="row">
        {{>layout/mypage-menu}}
        <div class="col-md-9">
            <div class="card">
                <div class="card-body">
                    <div class="row">
                        <div class="col-md-12">
                            <div class="col-md-9">
                                {{#hiker}}
                                    <div class="form-group">
                                        <form>
                                            <div class="form-group">
                                                <label for="name">name</label>
                                                <input type="text" class="form-control" value="{{lastname}} {{firstname}}" id="lastname" readonly>
                                            </div>
                                            <div class="form-group">
                                                <label for="ssn">ssn</label>
                                                <input type="text" class="form-control" value="{{ssn}}" id="ssn" readonly>
                                            </div>
                                            <div class="form-group">
                                                <label for="age">age</label>
                                                <input type="text" class="form-control" value="{{age}}" id="age" readonly>
                                            </div>
                                            <div class="form-group">
                                                <label for="sex">sex</label>
                                                <input type="text" class="form-control" value="{{sex}}" id="sex" readonly>
                                            </div>
                                            <div class="form-group">
                                                <label for="phone">phone</label>
                                                <input type="text" class="form-control" value="{{phone}}" id="phone" readonly>
                                            </div>
                                        </form>
                                    </div>
                                {{/hiker}}
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="/js/app/register.js"></script>
{{>layout/footer}}
